# One-way ANOVA
# Importing the dataset
PlantDataset <- PlantGrowth
View(PlantDataset)
# importing the dplyr library
library(dplyr)
library(plyr)
# importing the dplyr library
library(dplyr)
# importing the dplyr library
install.packages("dplyr")
library(dplyr)
stats <- PlantDataset %>%
group_by(group) %>%
summarise(
mean = mean(weight),
sd = sd(weight),
se = sd(weight) / sqrt(n())
)
View(stats)
print(stats)
boxplot(weight ~ group, data = PlantGrowth,
main = "PlantGrowth data",
ylab = "Dried weight of plants",
col = "lightgray"
)
# Mean plots visualization
install.packages("ggpubr")
# Mean plots visualization
# install.packages("ggpubr")
library(ggpubr)
ggline(PlantGrowth,
x = "group",
y = "weight",
add = c("mean_se", "jitter")
)
# Running the One-way ANOVA test to determine
# if there is any sig diff among the group means
anova <- aov(weight ~ group, data = PlantDataset)
View(stats)
summary(anova)
